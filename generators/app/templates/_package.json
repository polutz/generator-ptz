{
  "name": "<%= appname %>",
  "version": "1.0.2",
  "description": "this is a Polutz Domain module.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint ./src/**/*.ts ./src/**/*.test.ts ./src/**/*.d.ts",
    "js": "gulp js",
    "pretest": "npm-run-all --parallel js lint",
    "mocha": "mocha ./dist/**/*.js --compilers js:babel-core/register --require babel-polyfill",
    "test": "nyc npm run mocha && nyc report --reporter=text-lcov > coverage.lcov && codecov --token=<%= codecovToken %>"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubAuthorProject %>.git"
  },
  "author": "angeloocana.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/<%= githubAuthorProject %>/issues"
  },
  "homepage": "https://github.com/<%= githubAuthorProject %>#readme",  
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}